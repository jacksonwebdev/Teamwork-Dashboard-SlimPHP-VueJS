{% import 'app/macro/form.twig' as form %}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="{{ asset('favicon.ico') }}" type="image/x-icon">

    <title>{% block title %}coolblueweb{% endblock %}</title>

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/css/application.min.css') }}">
    {% endblock %}
</head>
<body>

    {% block flash %}
        {% include 'app/_flash.twig' %}
    {% endblock %}

    {% block body %}{% endblock %}

    {% block javascript %}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <script src="{{ asset('assets/js/application.min.js') }}"></script>
    {% endblock %}
</body>
</html>
